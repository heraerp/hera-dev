{
  "name": "hera-universal",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "claude": "../scripts/claude-with-context.sh",
    "update-context": "node ai-context-system/index.js --update-registry",
    "scan-codebase": "node ai-context-system/index.js --scan",
    "claude-copy": "../scripts/claude-context-copy.sh",
    "claude-context": "cat ../docs/ai-context/HERA-MASTER-CONTEXT.md",
