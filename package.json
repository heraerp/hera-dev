    "test:schema": "node scripts/run-tests.js --schema-only",
    "test:db": "node scripts/run-tests.js --database-only",
    "check-imports": "node scripts/check-imports.js",
    "fix-imports": "node scripts/fix-missing-imports.js",
    "validate": "npm run check-imports && npm run lint",
    "test:full": "node scripts/run-tests.js --full-suite",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "test:auth": "playwright test tests/auth-flow.spec.ts",
    "test:e2e:codegen": "playwright codegen http://localhost:3000/setup/restaurant",
    "test:login": "playwright test tests/e2e/login.spec.ts",
    "test:login:headed": "playwright test tests/e2e/login.spec.ts --headed",
    "test:login:debug": "playwright test tests/e2e/login.spec.ts --debug",
    "test:auth-setup": "playwright test tests/e2e/setup-auth.ts",
