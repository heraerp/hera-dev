    "test:ai:debug": "playwright test tests/master-ai-test-suite.spec.ts --debug",
    "test:ai:headed": "playwright test tests/master-ai-test-suite.spec.ts --headed",
    "test:visual": "playwright test tests/master-ai-test-suite.spec.ts --grep \"Visual AI Analysis\"",
    "test:architecture": "playwright test tests/master-ai-test-suite.spec.ts --grep \"HERA Architecture Compliance\"",
    "test:integration": "playwright test tests/master-ai-test-suite.spec.ts --grep \"Integration System Health\"",
    "test:master-suite": "playwright test tests/master-ai-test-suite.spec.ts --grep \"Execute Complete HERA Universal Test Suite\"",
    "test:users": "node scripts/demo-test-users.js",
    "test:users:examples": "playwright test tests/examples/test-user-examples.spec.ts --headed",
    "test:users:restaurant": "playwright test tests/restaurant-flow-with-test-users.spec.ts --headed",
    "test:users:quick": "playwright test tests/examples/test-user-examples.spec.ts -g \"Create and login with a complete test user\" --headed",
    "test:users:cleanup": "playwright test tests/examples/test-user-examples.spec.ts -g \"cleanup and recreation\" --headed",
    "test:users:load": "playwright test tests/examples/test-user-examples.spec.ts -g \"Load test with multiple users\" --headed",
    "type-check": "tsc --noEmit",
    "ci:check": "npm run type-check && npm run lint && npm run build",
    "ci:quick": "npm run type-check && npm run build",
    "setup:hooks": "../scripts/setup-git-hooks.sh",
    "hera:help": "node scripts/hera-dev-helper.js",
    "hera:watch": "node scripts/watch-architecture.js",
    "hera:fix": "node scripts/quick-fix.js",
    "hera:check": "node scripts/validate-architecture.js",
    "sw:unregister": "node scripts/sw-utils.js unregister",
    "sw:status": "node scripts/sw-utils.js status",
    "sw:clear": "node scripts/sw-utils.js clear",
    "sw:version": "node scripts/sw-utils.js version",
    "sw:help": "node scripts/sw-utils.js help"
